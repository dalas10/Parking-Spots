{
  "expo": {
    "name": "Urbee",
    "slug": "urbee",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#fdb82e"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.urbee.app",
      "config": {
        "googleMapsApiKey": "YOUR_GOOGLE_MAPS_API_KEY"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We need your location to find parking spots near you.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "We need your location to find parking spots near you."
      }
    },
    "android": {
      "package": "com.urbee.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "backgroundColor": "#fdb82e"
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "YOUR_GOOGLE_MAPS_API_KEY"
        }
      }
    },
    "plugins": [
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.urbee.app",
          "enableGooglePay": true
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Urbee to use your location to find parking near you."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Urbee to access your photos to upload parking spot images."
        }
      ]
    ]
  }
}
